{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "resolveJsonModule": true,
    "types": [
      "bun-types"
    ]
  },
  "include": [
    "src"
  ]
}